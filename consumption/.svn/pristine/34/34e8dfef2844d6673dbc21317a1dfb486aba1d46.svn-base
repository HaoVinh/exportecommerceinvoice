	public void capnhatiddonhang() {
		int sodonhangcnduoc = 0;
		for (int i = 0; i < listInvoice.size(); i++) {
			Invoice iv = invoiceService.findById(listInvoice.get(i).getId());
			if (iv.getIdorderlix() == 0) {
				List<InvoiceDetail> listInvoiceDetail = new ArrayList<>();
				invoiceDetailService.selectByInvoice(listInvoice.get(i).getId(), listInvoiceDetail);
				long idchitietdonhang = 0;
				for (InvoiceDetail dt : listInvoiceDetail) {
					if (dt.getDetail_own_id() != null && dt.getDetail_own_id() != 0) {
						idchitietdonhang = dt.getDetail_own_id();
						break;
					}
				}
				if (idchitietdonhang != 0) {
					InvoiceDetail od = invoiceDetailService.selectById(idchitietdonhang);
					if (od != null && od.getInvoice().getIdorderlix() != 0) {
						iv.setOrder_voucher(od.getInvoice().getOrder_voucher());
						iv.setIdorderlix(od.getInvoice().getIdorderlix());
						invoiceService.updateDonHang(iv);
						sodonhangcnduoc++;
					}
				}
			}
		}
		notice("Cap nhat duoc: " + sodonhangcnduoc + " don hang.");
	}

