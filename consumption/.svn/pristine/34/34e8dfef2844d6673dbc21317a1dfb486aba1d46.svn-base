					listWrapInvoiceDetail.add(w);
				}
				// tính tổng tiền hóa đơn
				// sumInvoice();
			}
		} catch (Exception e) {
			logger.error("InvoiceBean.nextOrPrev:" + e.getMessage(), e);
		}
		current.executeScript("PF('tablect').clearFilters();");
	}

