	public void nextOrPrev(int next) {
		PrimeFaces current = PrimeFaces.current();
		try {
			if (listInvoice != null && listInvoice.size() > 0) {

				int index = -1;
				for (int i = 0; i < listInvoice.size(); i++) {
					if (listInvoice.get(i).getId() == invoiceCrud.getId()) {
						index = i;
						break;
					}
				}
				int size = listInvoice.size() - 1;
				if (index == -1) {
					invoiceSelect = listInvoice.get(0);
					invoiceCrud = invoiceService.findById(invoiceSelect.getId());
				} else {
					switch (next) {
					case 1:
						if (index == size) {
							warning("Hết danh sách");
