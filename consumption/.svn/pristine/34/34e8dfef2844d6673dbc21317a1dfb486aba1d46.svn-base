							warning("Hết danh sách");
							// invoiceSelect = listInvoice.get(size);
							// invoiceCrud = invoiceSelect.clone();
						} else {
							invoiceSelect = listInvoice.get(index - 1);
							invoiceCrud = invoiceService.findById(invoiceSelect.getId());
						}
						break;
					}
				}
