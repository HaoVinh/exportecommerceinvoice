							invoiceDetailService.selectByInvoice(invs.get(i).getId(), invoiceDetails);
							for (int j = 0; j < invoiceDetails.size(); j++) {
								double kl = (invoiceDetails.get(j).getQuantity() * invoiceDetails.get(j).getProduct()
										.getFactor());
								khoiluong += kl;
