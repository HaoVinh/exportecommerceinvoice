			jsonInfo.add("status", array);
			JsonObject json = new JsonObject();
			json.add("invoice_info", jsonInfo);

			long startTime = System.currentTimeMillis();
			invoiceService.search(JsonParserUtil.getGson().toJson(json), listInvoice, customerChannelSearchs,
					customerTypesSearchs, chuachuyenmisa, giohanHDtruyvan, ieCategoriesSearchs, customerSearchs);
			long endTime = System.currentTimeMillis();
			long duration = endTime - startTime;
			System.out.println("Thời gian thực thi: " + duration + " mili giây");

		} catch (Exception e) {
			logger.error("InvoiceBean.search:" + e.getMessage(), e);
		}
	}

