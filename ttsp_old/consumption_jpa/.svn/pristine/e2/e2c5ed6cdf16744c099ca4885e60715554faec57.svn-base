package lixco.com.reportInfo;

public class TonKhoDauThang {
	private long product_id;
	private double unit_opening_balance;
	private double kg_opening_balance;
	private double kg_inv_balance;
	
	public TonKhoDauThang() {
	}
	
	public TonKhoDauThang(long product_id,  double unit_opening_balance, double kg_opening_balance) {
		this.product_id = product_id;
		this.unit_opening_balance = unit_opening_balance;
		this.kg_opening_balance = kg_opening_balance;
	}

	public long getProduct_id() {
		return product_id;
	}

	public void setProduct_id(long product_id) {
		this.product_id = product_id;
	}

	public double getUnit_opening_balance() {
		return unit_opening_balance;
	}

	public void setUnit_opening_balance(double unit_opening_balance) {
		this.unit_opening_balance = unit_opening_balance;
	}

	public double getKg_opening_balance() {
		return kg_opening_balance;
	}

	public void setKg_opening_balance(double kg_opening_balance) {
		this.kg_opening_balance = kg_opening_balance;
	}

	public double getKg_inv_balance() {
		return kg_inv_balance;
	}

	public void setKg_inv_balance(double kg_inv_balance) {
		this.kg_inv_balance = kg_inv_balance;
	}


	
}
