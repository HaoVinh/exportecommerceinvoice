package lixco.com.reportInfo;

import lombok.Data;

@Data
public class TongKetTheoSanPham {
	private long product_type_id;
	private String product_type_name;
	private long product_brand_id;
	private String product_brand_name;
	private String product_com_code;
	private String product_com_name;
	private Double quantity;
	private Double total_amount;
	private Double total_tax_amount;
	private Double total_amount_with_vat;
	
	private String ie_categories_name;
	
	public TongKetTheoSanPham() {
	}
	
	public TongKetTheoSanPham(long product_type_id, String product_type_name, long product_brand_id,
			String product_brand_name, Double quantity, Double total_amount, Double total_tax_amount,
			Double total_amount_with_vat) {
		this.product_type_id = product_type_id;
		this.product_type_name = product_type_name;
		this.product_brand_id = product_brand_id;
		this.product_brand_name = product_brand_name;
		this.quantity = quantity;
		this.total_amount = total_amount;
		this.total_tax_amount = total_tax_amount;
		this.total_amount_with_vat = total_amount_with_vat;
	}

	
}
