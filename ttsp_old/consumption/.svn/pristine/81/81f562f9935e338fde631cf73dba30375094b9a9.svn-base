<h:body>
	<!--       <p:ajaxStatus onstart="Pace.start()" /> -->
	<p:ajaxStatus styleClass="statusprocess" id="ajaxStatusPanel">
		<f:facet name="start">
			<div class="contentloading">
				<span class="loader"></span> <span
					style="font-weight: bold; color: white;">Đang xử lý...</span>
			</div>
		</f:facet>
		<f:facet name="complete">
			<h:outputText value="" />
		</f:facet>
	</p:ajaxStatus>
	<h:form>
		<p:dialog widgetVar="thongbao" styleClass="noticedialog"
			header="Thông báo" style="height:10px" resizable="false">
			<h:outputText value="Không có dữ liệu phát sinh!"
				style="color: red;font-size: 14px;" />
			<br />
			<p:separator />
			<p:commandButton value="OK" style="margin-left:50px"
				onclick="PF('thongbao').hide();" styleClass="ui-confirmdialog-yes"
				icon="ui-icon-check" />
		</p:dialog>
	</h:form>
	<h:form id="messages">
		<p:growl escape="true" life="5000" showDetail="true" />
	</h:form>
	<h:form style="display:none;">
		<p:confirmDialog header="Thông báo" global="true" showEffect="fade"
			hideEffect="fade" styleClass="confirmDialog" closeOnEscape="true"
			message="Bạn có chắc muốn xoá không?">
			<p:commandButton value="OK" type="button"
				styleClass="ui-confirmdialog-yes" icon="ui-icon-check" />
			<p:commandButton value="Cancel" type="button"
				styleClass="ui-confirmdialog-no " icon="ui-icon-close" />
		</p:confirmDialog>
	</h:form>
	<div class="wrapper">
		<h:form>
			<header class="main-header"> <!-- Logo -->
			<div class="logo">
				<span class="logo-lg"><b><p:commandLink
							action="#{applicationBean.logoutHome()}"
							style="font-size: unset;    color: #ccd8de;" value="LIXCO" /></b></span>
			</div>
			<nav class="navbar navbar-static-top"> <a href="#"
				class="sidebar-toggle" data-toggle="push-menu" role="button"> <span
				class="sr-only">Toggle navigation</span>
			</a> <!-- Sidebar toggle button-->
			<div class="navbar-custom-menu">
				<ul class="nav navbar-nav">
					<li class="dropdown user user-menu"><a href="#"
						class="dropdown-toggle" data-toggle="dropdown"> <img
							src="#{request.contextPath}/resources/icon/user3-128x128.jpg"
							class="user-image" alt="User Image" /> <span class="hidden-xs"><h:outputText
									value="#{sessionScope.account.member.name}" /> </span>
					</a>
						<ul class="dropdown-menu">
							<!-- User image -->
							<li class="user-header"><img
								src="#{request.contextPath}/resources/icon/user3-128x128.jpg"
								class="img-circle" alt="User Image" />
								<p>
									<h:outputText value="#{sessionScope.account.member.name}" />
									<small><h:outputText value="" /></small>
								</p></li>
							<!-- Menu Body -->
							<!-- Menu Footer-->
							<li class="user-footer">
								<div class="pull-left">
									<a href="#" class="btn btn-default btn-flat">Thông tin</a>
								</div>
								<div class="pull-right">
									<h:commandLink styleClass="btn btn-default btn-flat"
										action="#{applicationBean.logout()}">
                     Đăng xuất
                     <f:ajax execute="@this" render="@this"></f:ajax>
									</h:commandLink>
								</div>
							</li>
						</ul></li>
					<!-- Control Sidebar Toggle Button -->
					<li style="display: none;"><a href="#"
						data-toggle="control-sidebar"><i class="fa fa-gears"></i></a></li>
				</ul>
			</div>
			</nav> </header>
		</h:form>
		<!-- Left side column. contains the logo and sidebar -->
		<aside class="main-sidebar"> <!-- sidebar: style can be found in sidebar.less -->
		<section class="sidebar"> <!-- Sidebar user panel --> <!-- /.search form -->
		<!-- sidebar menu: : style can be found in sidebar.less --> <h:form>
			<h:outputText value="#{menuBean2.menuStr}" escape="false" />
		</h:form> </section> <!-- /.sidebar --> </aside>

		<!-- Content Wrapper. Contains page content -->
