package lixco.com.reqInfo;

import java.util.List;

import lixco.com.entity.PromotionOrderDetail;

public class WrapExtensionOrderReqInfo {
	private long order_id;
	private long member_id;
	private String member_name;
	private List<WrapOrderDetailLixReqInfo> list_wrap_extension_order_detail;
	private List<PromotionOrderDetail> list_promotion_order_detail;
	
	public WrapExtensionOrderReqInfo() {
	}
	
	

	public WrapExtensionOrderReqInfo(long order_id, long member_id, String member_name,
			List<WrapOrderDetailLixReqInfo> list_wrap_extension_order_detail,
			List<PromotionOrderDetail> list_promotion_order_detail) {
		this.order_id = order_id;
		this.member_id = member_id;
		this.member_name = member_name;
		this.list_wrap_extension_order_detail = list_wrap_extension_order_detail;
		this.list_promotion_order_detail = list_promotion_order_detail;
	}



	public long getOrder_id() {
		return order_id;
	}
	public void setOrder_id(long order_id) {
		this.order_id = order_id;
	}
	public long getMember_id() {
		return member_id;
	}

	public void setMember_id(long member_id) {
		this.member_id = member_id;
	}

	public String getMember_name() {
		return member_name;
	}
	public void setMember_name(String member_name) {
		this.member_name = member_name;
	}
	public List<WrapOrderDetailLixReqInfo> getList_wrap_extension_order_detail() {
		return list_wrap_extension_order_detail;
	}
	public void setList_wrap_extension_order_detail(List<WrapOrderDetailLixReqInfo> list_wrap_extension_order_detail) {
		this.list_wrap_extension_order_detail = list_wrap_extension_order_detail;
	}

	public List<PromotionOrderDetail> getList_promotion_order_detail() {
		return list_promotion_order_detail;
	}

	public void setList_promotion_order_detail(List<PromotionOrderDetail> list_promotion_order_detail) {
		this.list_promotion_order_detail = list_promotion_order_detail;
	}
}
