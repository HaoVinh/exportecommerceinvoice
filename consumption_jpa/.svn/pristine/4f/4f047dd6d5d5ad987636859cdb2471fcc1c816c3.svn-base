package lixco.com.entityapi;

import lixco.com.entity.ProductType;
import lombok.Data;
@Data
public class ProductTypeDTO{
	private long id;
	private String code;
	private String name;
	private String en_name;
	private int typept;
	
	 public ProductTypeDTO(ProductType pt){
	        this.code = pt.getCode();
	        this.name = pt.getName();
	        this.en_name = pt.getEn_name();
	        this.typept = pt.getTypept();
	    }
	
}
