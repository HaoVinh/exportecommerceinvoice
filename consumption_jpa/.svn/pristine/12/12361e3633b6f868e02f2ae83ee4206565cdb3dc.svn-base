		this.malohang = malohang;
	}
	public String getThutuhd() {
		return thutuhd;
	}
	public void setThutuhd(String thutuhd) {
		this.thutuhd = thutuhd;
	}
	public Date getUpdatetime() {
		return updatetime;
	}
	public void setUpdatetime(Date updatetime) {
		this.updatetime = updatetime;
	}
